
#include<stdio.h>
#include<conio.h>
main()
{
	float celsius, fahrenheit;

	float baseSalary, hra, da, ta;
	float hraPercent, daPercent, taPercent;
	float grossSalary, hraAmount, daAmount, taAmount;

	float angle1, angle2, angle3;

	clrscr();

	printf("Q:1 Temperature converter :-\n");
	printf("\n");

	printf("The temperature in celsius :");
	scanf("%f", &celsius);
	fahrenheit = (9.0 / 5.0) * celsius + 32;
	printf("Temperature in fahrenheit : %f\n",fahrenheit);

	printf("\n");
	printf("\n");
	printf("\n");
	printf("\n");

	printf("Q:2 Temperature converter :-\n");


	printf("Enter :- Base salary: ");
	scanf("%f", &baseSalary);

	printf("         HRA:");
	scanf("%f", &hraPercent);

	printf("         DA:");
	scanf("%f", &daPercent);

	printf("         TA:");
	scanf("%f", &taPercent);

	hraAmount = (hraPercent / 100) * baseSalary;
	daAmount = (daPercent / 100) * baseSalary;
	taAmount = (taPercent / 100) * baseSalary;

	grossSalary = baseSalary + hraAmount + daAmount + taAmount;

	printf("Gross Salary : Rs. %.0f\n ",grossSalary);

	printf("\n");
	printf("\n");
	printf("\n");
	printf("\n");

	printf("Q:3 Triangle Angle Finder :-\n");

	printf("First Angle;-");
	scanf("%f", &angle1);

	printf("Second Angle:-");
	scanf("%f", &angle2);

	angle3 = 180 - (angle1 + angle2);

	printf("Third Angle is :- %f\n",angle3);

	getch();
	return 0;
}








